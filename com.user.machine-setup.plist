<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.user.machine-setup</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/zsh</string>
      <string>-c</string>
      <string>/bin/zsh -c "$(curl -fsSL https://raw.githubusercontent.com/Macca2805/machine-setup/main/setup.sh)"</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/com.user.machine-setup.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.machine-setup.log</string>
  </dict>
</plist>